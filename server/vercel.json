{
    "version": 2,
    "name":"node-js",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" ,
        "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }
    }
    ]
  }